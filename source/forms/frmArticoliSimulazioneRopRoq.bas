Dim Costo_unit_ord As Variant
Dim RoqResult As Variant
Dim DatiGen As New ADODB.Recordset
Dim conn As ADODB.Connection
Set db0 = CurrentDb
Set conn = CurrentProject.Connection

'memorizzo il Costo di Acquisto
DatiGen.Open "tblDatiGenerali", conn, adOpenKeyset, adLockOptimistic
    Costo_unit_ord = DatiGen.Fields("Costo_Unit_Ord")
DatiGen.Close

'Formula Lotto Economico
        RoqResult = Sqr((2 * (AvgConsumoMese * 12) * Costo_unit_ord) / (Me.txtCs_CscSimu * 0.68 * 0.21))
    If RoqResult > 0 And RoqResult < 1 Then
        RoqSimu = 1
    Else
        RoqSimu = Round(RoqResult, 0)
    End If
End Function

