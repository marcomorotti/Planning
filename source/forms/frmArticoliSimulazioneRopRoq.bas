Function ScortaSicurezzaSimu(LivelloServizio, Lead_time, AvgConsumoMese, _
    DevStdConsumoMese) As Variant
' Inserire Lead_Time[mese]
Dim SSResult As Variant
Dim Lead_Time_Mese As Variant
Lead_Time_Mese = Lead_time / 22
If LivelloServizio < 50 Then
    ScortaSicurezzaSimu = 0
Else
    SSResult = InverseCDF(LivelloServizio / 100) * (Sqr(Lead_Time_Mese * _
            (DevStdConsumoMese) ^ 2))
    ScortaSicurezzaSimu = Round(SSResult, 0)
End If
End Function
