Dim conn As ADODB.Connection
Dim DatiGen As New ADODB.Recordset
Set conn = CurrentProject.Connection
    DatiGen.Open "tblDatiGenerali", conn, adOpenKeyset, adLockOptimistic
    intYear = DatiGen.Fields("Anno_Calcolo")
    intMonth = DatiGen.Fields("Mese_Calcolo")
    ' intLastDay = Day(DateAdd("m", 1, DateSerial(intYear, intMonth, 1)) - 1)
    txtBuild = DateSerial(intYear, intMonth, 1)  'Setta data ricerca su campo nascosto per qry Dati Maschera
    ' txtBuild = DateSerial(Year(Date), Month(Date), 1)     'Setta data ricerca su campo nascosto per qry Dati Maschera
DatiGen.Close

    Requery  'RILANCIA LA QUERY
    ' Cancella tutte le caselle
    For intI = 1 To 17
