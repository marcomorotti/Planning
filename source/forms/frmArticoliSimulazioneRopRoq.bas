Function RopSimu(LivelloServizio, Lead_time, _
    AvgConsumoMese, DevStdConsumoMese) As Variant

' Inserire Lead_Time[mese]
' E' possibile anche usare il ModInvCDF
Dim RopResult As Variant
Dim Lead_Time_Mese As Variant
Lead_Time_Mese = Lead_time / 22
RopResult = (Lead_Time_Mese * AvgConsumoMese)
If RopResult > 0 And RopResult < 1 Then
        RopSimu = 1
    Else
        RopSimu = Round(RopResult, 0)
    End If
End Function
