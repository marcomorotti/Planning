Private Sub cmdImportData_Click()
Dim strSQL As String
Dim QI As Integer
Dim HasHeader As Boolean
Dim MyQuery As String
QI = MsgBox("Confermi che vuoi aggiornare il Data Base? : ", vbYesNo)
If QI = vbNo Then Exit Sub
FileConsumi = Me.txtFileConsumi.Value
FileGiacenze = Me.txtFileGiacenze.Value
HasHeader = Forms!frmImportDatiGen!HasHeader
' ***************
' Carica Consumi
' ***************
If IsNull(FileConsumi) Or FileConsumi = "" Then
    MsgBox "Devi scegliere il File Consumi prima!"
    Me.cmdBrowseConsumi.SetFocus
    Exit Sub
Else
        ' Verifica che il File Esista
        If FileExists(FileConsumi) Then
            ' Cancella dati dalla tabella temporanea tblImportSpedito e carica file Excel
            ImportaFile FileConsumi, "tblImportSpedito", HasHeader
            ' Conta n. record caricati
            MyQuery = "SELECT * from tblImportSpedito"
            n_rec = GetSQLRecordcount(MyQuery)
            MsgBox "Importazione Consumi Terminata. Caricati  " & n_rec & "  record."
        Else
           MsgBox "Il File Consumi non esiste. Usa Browse e scegli nuovo File!!!"
           Me.cmdBrowseConsumi.SetFocus
           Exit Sub
        End If
End If
' ***************
' Carica Giacenze
' ***************
If IsNull(FileGiacenze) Or FileGiacenze = "" Then
    MsgBox "Devi scegliere il File Giacenze prima! - NB LE GIAC NON SONO NECESSARIE!!!"
    Me.cmdBrowseGiacenze.SetFocus
    Exit Sub
Else
        ' Verifica che il File Esista
        If FileExists(FileGiacenze) Then
            ' Cancella dati dalla tabella temporanea tblImportSpedito e carica file Excel
            ImportaFile FileGiacenze, "tblImportGiacenza", HasHeader
            ' Conta n. record caricati
            MyQuery = "SELECT * from tblImportGiacenza"
            n_rec = GetSQLRecordcount(MyQuery)
            MsgBox "Importazione Giacenze Terminata. Caricati  " & n_rec & "  record."
        Else
           MsgBox "Il File Giacenze non esiste. Usa Browse e scegli nuovo File!!!"
           Me.cmdBrowseGiacenze.SetFocus
           Exit Sub
        End If
End If
End Sub

