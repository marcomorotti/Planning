'Inizia routine di ricerca
Private Sub cmdSearch_Click()
Dim Varwhere As Variant, varDateSearch As Variant
Dim rst As DAO.Recordset

    ' Initialize to Null
    Varwhere = Null
    varDateSearch = Null
'Marco Morotti 12.08.2010
'---- Inizia routine di riempimento varibile varWhere
    ' Cerca CodiceArticolo
    If Not IsNothing(Me!txtCod_Art) Then
      Varwhere = (Varwhere + " AND ") & "[Cod_Art] LIKE '" & Me.txtCod_Art & "*'"
    End If
    ' Do N Ordine next
    If Not IsNothing(Me.txtNumero_Doc) Then
        ' .. build the predicate
        ' Note: taking advantage of Null propogation
        '  so we don't have to test for any previous predicate
        Varwhere = (Varwhere + " AND ") & "[Numero_Doc] LIKE '" & Me.txtNumero_Doc & "*'"
    End If
    
    ' Do Codice Cliente next
    If Not IsNothing(Me.txtCod_Cli) Then
        ' .. build the predicate
        ' Note: taking advantage of Null propogation
        '  so we don't have to test for any previous predicate
        Varwhere = (Varwhere + " AND ") & "[Cod_Cli] LIKE '" & Me.txtCod_Cli & "*'"
    End If
    
    ' Do Ragione Sociale next
    If Not IsNothing(Me.txtDs_Rag_Soc) Then
        ' .. build the predicate
        ' Note: taking advantage of Null propogation
        '  so we don't have to test for any previous predicate
        Varwhere = (Varwhere + " AND ") & "[Ds_Rag_Soc] LIKE '" & Me.txtDs_Rag_Soc & "*'"
    End If
    
     ' Do Utente next
    If Not IsNothing(Me.txtUtente) Then
        ' .. build the predicate
        ' Note: taking advantage of Null propogation
        '  so we don't have to test for any previous predicate
        Varwhere = (Varwhere + " AND ") & "[Utente] LIKE '" & Me.txtUtente & "*'"

    End If
    
 '---- Fine routine di riempimento varibile varWhere
 '----
    ' Check to see that we built a filter
    If IsNothing(Varwhere) Then
        MsgBox "Devi inserire almeno un parametro di ricerca.", vbInformation, gstrAppTitle
        Exit Sub
    End If
    
    ' Open a recordset to see if any rows returned with this filter
    Me.Visible = False
    Set rst = DBEngine(0)(0).OpenRecordset("SELECT * FROM tblCorders WHERE " & Varwhere)
    ' See if found none
    If rst.RecordCount = 0 Then
        MsgBox "Nessun Ordine trovato.", vbInformation, gstrAppTitle
        ' Clean up recordset
        rst.Close
        Set rst = Nothing
        Exit Sub
    End If
    
    ' Hide me to fix later focus problems
'    Me.Visible = False
    ' Move to last to find out how many
    rst.MoveLast
    DoCmd.OpenForm "frmOcsaMstSearch", WhereCondition:=Varwhere
        ' Make sure focus is on Articoli
        Forms!frmOcsaMstSearch.SetFocus
    

    ' Done
'    DoCmd.Close acForm, Me.Name
    ' Clean up recordset
    rst.Close
    Set rst = Nothing

End Sub

Private Sub txtCod_Art_Enter()
Call cmdSearch_Click
End Sub
