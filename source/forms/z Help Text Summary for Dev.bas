Private Sub CmdView_Click()
   ' Initialize to Null
   Dim Varwhere As Variant
    Varwhere = Null
    If Not IsNothing(Me.zhID) Then
        ' .. build the predicate
        ' Note: taking advantage of Null propogation
        '  so we don't have to test for any previous predicate
        Varwhere = "[zhID] = " & Me.zhID
    End If
  
  If (gintIsAdmin = True) Then
        DoCmd.OpenForm "z Help Text", WhereCondition:=Varwhere
        Exit Sub
    End If
    ' If someone signed on with Dept. ID,
    If glngThisDeptID <> 0 Then
        ' Then filter for just that department
        DoCmd.OpenForm "z Help Text", WhereCondition:=Varwhere
    Else
        ' Open 'em all
        DoCmd.OpenForm "z Help Text", WhereCondition:=Varwhere
    End If
End Sub
