Private Sub txtModello_KeyPress(KeyAscii As Integer)
'----------------------------------------------------------------
'build the filter text and filter the listbox
'characters do not get added to txtName until user tabs out
'so must use another textbox on the form to build the characters

    If KeyAscii = 8 Then                        'backspace key pressed - Ascii code 8

        If Len(txtBuildName) >= 1 Then       'is there is a character to remove?
            txtBuildName = left(txtBuildName, Len(txtBuildName) - 1)  'delete end char
        End If
    Else
        txtBuildName = txtBuildName & Chr(KeyAscii)  'add new character to the string
    End If
    
    lstMember.Requery
    
End Sub

