Private Sub lstMember_DblClick(Cancel As Integer)
'-----------------------------------------
'Open the Membership form showing this member

    If lstMember.ListCount = 0 Then
    'do nothing - list box is empty
    Else
        'call wizard code to open the form for this member
        cmdOpenMember_Click
    End If
End Sub

